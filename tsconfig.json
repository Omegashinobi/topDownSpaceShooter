{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "esnext",
      "target": "ES2017",
      "allowJs": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "downlevelIteration": true,
      "resolveJsonModule": true,
      "esModuleInterop": true
    },
    "exclude": ["projectFiles"]
  }